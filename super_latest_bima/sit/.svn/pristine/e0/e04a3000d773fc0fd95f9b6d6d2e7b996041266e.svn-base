package id.gnvsolution.d3.api.dto.response;

import id.gnvsolution.d3.api.dto.DTO;

public class BonstriMonsterDto implements DTO {
    private String monsterName;

    private String monsterCount;

    private String monsterPoint;

    public String getMonsterName() {
        return monsterName;
    }

    public void setMonsterName(String monsterName) {
        this.monsterName = monsterName;
    }

    public String getMonsterCount() {
        return monsterCount;
    }

    public void setMonsterCount(String monsterCount) {
        this.monsterCount = monsterCount;
    }

    public String getMonsterPoint() {
        return monsterPoint;
    }

    public void setMonsterPoint(String monsterPoint) {
        this.monsterPoint = monsterPoint;
    }
}
