<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <tlib-version>1.0</tlib-version>
    <function>
        <description>Exposes the abbreviate() function from Apache StringUtils</description>
        <name>abbreviate</name>
        <function-class>org.apache.commons.lang.StringUtils</function-class>
        <function-signature>java.lang.String abbreviate(java.lang.String, int)</function-signature>
    </function>
    
    <function>
        <description>Convert from Bytes to GB </description>
        <name>bytes2GB</name>
        <function-class>com.gnv.vnm.selfcare.core.utils.UnitConverter</function-class>
        <function-signature>java.lang.String bytes2GB(long)</function-signature>
    </function>
    
    <function>
        <description>Convert milliseconds to hh:mm:ss </description>
        <name>milisToString</name>
        <function-class>id.gnvsolution.d3.api.utils.UnitConverter</function-class>
        <function-signature>java.lang.String milisToString(long, java.lang.String)</function-signature>
    </function>    
        
</taglib>