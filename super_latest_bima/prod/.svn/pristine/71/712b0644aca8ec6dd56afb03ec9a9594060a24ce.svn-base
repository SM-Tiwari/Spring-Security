package id.gnvsolution.d3.api.dto.request;

import id.gnvsolution.d3.api.Status;

/**
 * Created by Arie
 * on 05/04/2017.
 */
public class ContactTriDto extends BaseRequestDto {
    private String name;
    private String serviceType;
    private Status status;
    private String replyPlatform;
    private String emailAddress;
    private String messageTitle;
    private String messageContent;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getServiceType() {
        return serviceType;
    }

    public void setServiceType(String serviceType) {
        this.serviceType = serviceType;
    }

    public Status getStatus() {
        return status;
    }

    public void setStatus(Status status) {
        this.status = status;
    }

    public String getReplyPlatform() {
        return replyPlatform;
    }

    public void setReplyPlatform(String replyPlatform) {
        this.replyPlatform = replyPlatform;
    }

    public String getEmailAddress() {
        return emailAddress;
    }

    public void setEmailAddress(String emailAddress) {
        this.emailAddress = emailAddress;
    }

    public String getMessageTitle() {
        return messageTitle;
    }

    public void setMessageTitle(String messageTitle) {
        this.messageTitle = messageTitle;
    }

    public String getMessageContent() {
        return messageContent;
    }

    public void setMessageContent(String messageContent) {
        this.messageContent = messageContent;
    }
}
